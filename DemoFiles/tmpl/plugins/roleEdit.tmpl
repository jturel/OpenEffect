<form method="POST" action="{_LINK_SELF}" name="mainForm">
  <input type="Hidden" name="UserID" value="{UserID}">
  <input type="Hidden" name="RoleID" value="{RoleID}">
<table width="100%" border="0" cellspacing="0" cellpadding="0" class="main">
  <tr> 
    <td> 
      <table width="100%" border="0" cellspacing="0" cellpadding="0" class="caption">
        <tr valign="middle" align="left"> 
          <td width="18"><img src="img/gn_access.gif" width="18" height="18"></td>
          <td>##Role##</td>
        </tr>
      </table>
    </td>
  </tr>
  <tr> 
    <td> 
      <table width="100%" border="0" cellspacing="10" cellpadding="0" height="100%" class="bg_color_light">
        <tr> 
          <td class="bg_color_light" align="left" valign="middle"> 
            <nobr>##Role## ID:</nobr>
          </td>
          <td class="bg_color_light" align="left" valign="middle"> 
            &nbsp;&nbsp;<b>{RoleID}</b>
          </td>
        </tr>
        <tr> 
          <td class="bg_color_light" align="left" valign="middle"> 
            ##Name##:
          </td>
          <td class="bg_color_light" align="left" valign="middle"> 
            <input type="Text" name="Name" value="{Name}" size="15" maxlength="10" class="input">
          </td>
        </tr>
      </table>
    </td>
  </tr>
  <tr>
    <td>
        <table width="100%" border="0" cellspacing="5" cellpadding="0">
          <tr> 
            <td> 
            <input type="Submit" name="save" value="##Save##" class="input">
            <input type="Button" value="##Cancel##" onClick="parent.close()" class="input">
            </td>
          </tr>
        </table>
    </td>
  </tr>
</table>
</form>
