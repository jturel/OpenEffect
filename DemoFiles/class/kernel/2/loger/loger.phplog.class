#
#  loger item Class (PHP log output)
# 
 

 class logerItemClass_phplog extends linkerClass {
    var $className = "logerItemClass_phplog"; // Class name

    // Send message to stream
    // &$str - pointer on string which will be send in output
    function put(&$str) {
       error_log("[".$this->parent->streamName."] $str", 0);
       return true;
    }
 }


